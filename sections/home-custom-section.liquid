    }

    #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.dt-sc-reverse-rows .dt-sc-custom-content.dt-sc-reverse-columns .dt-sc-heading {
