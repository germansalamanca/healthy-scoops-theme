    max-width: 40%; height: auto;
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block:not(.dt-sc-reverse-rows) .dt-sc-custom-content {
   transform: translateY(2rem) translateX(6rem); -webkit-transform: translateY(2rem) translateX(6rem); 
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block:not(.dt-sc-reverse-rows) .dt-sc-custom-content .dt-sc-heading {
    transform: translateX(-100px); -webkit-transform: translateX(-100px);
  }
    
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block:not(.dt-sc-reverse-rows) .dt-sc-custom-content.dt-sc-reverse-columns {
   transform: translateY(2rem) translateX(-6rem);  -webkit-transform: translateY(2rem) translateX(-6rem); 
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block:not(.dt-sc-reverse-rows) .dt-sc-custom-content.dt-sc-reverse-columns .dt-sc-heading {
    transform: translateX(100px); -webkit-transform: translateX(100px);
