    transform: translateX(100px); -webkit-transform: translateX(100px);
  }    
    
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.dt-sc-reverse-rows:first-child:not(:only-child) {
    padding-left: 10rem; transform: translateY(0rem); -webkit-transform: translateY(0rem);
  }
    
	#shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.dt-sc-reverse-rows:first-child 
    + .dt-sc-custom-block:not(.dt-sc-reverse-rows):last-child {
    padding-right: 10rem;
  }
    
  
  {% endif %}
    
  }
  
  
  @media (min-width: 992px) and (max-width: 1200px) {
    
    #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block:not(.dt-sc-reverse-rows) .dt-sc-custom-content .dt-sc-heading {
      transform: translateX(-150px); -webkit-transform: translateX(-150px); width: 120%;
    }
    
