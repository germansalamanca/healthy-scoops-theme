{%- liquid
  assign margin = section.settings.margin | split: ',' 
  assign margin_top = margin[0]
  assign margin_bottom = margin[1]  
  assign padding = section.settings.padding | split: ',' 
  assign padding_top = padding[0] 
  assign padding_bottom = padding[1] 
  if section.blocks.size > 0 
  if section.settings.enable_overlay 
  assign section_overlay = 'dt-sc-overlay' 
  endif 

  if section.settings.show_background_image 
  assign lazyScript = "lazyload" 
  endif
  endif
 %}
<div class="dt-sc-section-wrapper  {{ lazyScript }} {{ section_overlay }} {% if section.settings.background_style == 'parallax' %} parallax-initiated{% endif %}" {% if section.settings.background_style == 'parallax' %} data-speed="2" data-style="parallax"{% endif %} style="{% render 'bg-color-image-padding' %} background-size: {{- section.settings.background_size -}}; {% if section.settings.background_style == 'fixed' %}background-attachment: fixed;{% endif %} background-repeat:{% if section.settings.background_size == 'repeat' %}repeat{% else %}no-repeat;{% endif %}" {% render 'bg_image' %}>  
  <div class="{% render 'container-settings' %}"> 
    <div class="row {{ section.settings.custom_class_name }}"> 
      {% render 'section-heading', class: section.id %} 
      <div class="home-custom-section-wrapper dt-sc-column {% if section.settings.two_column %} two-column {% endif %} {% if section.settings.custom_style == 'custom_style_2' %} custom_style{% endif %}">
        {% for block in section.blocks %}
        <div class="wow fadeIn dt-sc-custom-block {{ block.type }}-{{ block.id }} {% if block.settings.reverse_row %}dt-sc-reverse-rows{% endif %}">
         
              {% if block.settings.block_heading != blank or block.settings.block_sub_heading != blank or block.settings.block_description != blank or block.settings.html != blank or block.settings.block_button_text or block.settings.block_button_link != blank or block.settings.block_img_1 != blank %}
              <div class="dt-sc-custom-content {{ block.settings.heading_position_vertical }} {% if block.settings.reverse_column %}dt-sc-reverse-columns{% endif %}"> 
                {% if block.settings.block_img_1 != blank %}
                <div class="dt-sc-custom-image dt-sc-block-image-one" >
                  <img  class="dt-sc-block-image"
                    srcset="{%- if block.settings.block_img_1.width >= 275 -%}{{ block.settings.block_img_1 | image_url: width: 275 }} 275w,{%- endif -%}
                    {%- if block.settings.block_img_1.width >= 550 -%}{{ block.settings.block_img_1 | image_url: width: 550 }} 550w,{%- endif -%}
                    {%- if block.settings.block_img_1.width >= 710 -%}{{ block.settings.block_img_1 | image_url: width: 710 }} 710w,{%- endif -%}
                    {%- if block.settings.block_img_1.width >= 1420 -%}{{ block.settings.block_img_1 | image_url: width: 1420 }} 1420w,{%- endif -%}
                    {{ block.settings.block_img_1 | image_url }} {{ block.settings.block_img_1.width }}w"
                    src="{{ block.settings.block_img_1 | image_url: width: 550 }}"
                    sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                    (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %}, calc(100vw - 30px)"
                    alt="{{ block.settings.block_img_1.alt }}"
                    height="{{ block.settings.block_img_1.height }}"
                    width="{{ block.settings.block_img_1.width }}"
                    loading="lazy"
                  >
                </div>
                {% endif %}
                {% if block.settings.block_heading != blank or block.settings.block_sub_heading != blank or block.settings.html != blank or block.settings.block_description != blank or block.settings.block_button_text or block.settings.block_button_link != blank %}
                <div class="dt-sc-heading {{ block.settings.heading_position }}">                
                  {%- if block.settings.block_sub_heading != blank -%}
                  <h6 class="dt-sc-sub-heading">{{ block.settings.block_sub_heading }}</h6>
                  {%- endif -%}
                  {%- if block.settings.block_heading != blank -%}
                  <h4 class="dt-sc-main-heading">{{ block.settings.block_heading }}</h4>
                  {%- endif -%}
                  {%- if block.settings.block_description != blank -%}
                  <p class="dt-sc-heading-description">{{ block.settings.block_description }}</p>
                  {%- endif -%}
                  {%- if block.settings.html != blank -%}
                  <div class="dt-sc-heading-html rte">{{ block.settings.html }}</div>
                  {%- endif -%}
                  {%- if block.settings.block_button_link != blank and block.settings.block_button_text != blank -%}
                  <a href="{{ block.settings.block_button_link }}" class="dt-sc-btn">{{ block.settings.block_button_text }}</a>
                  {%- endif -%}
                </div>
                {% endif %}
          </div> 

          {% if block.settings.block_img_2 != blank %}
          <div class="dt-sc-block-image-two">
            <img  class="dt-sc-block-image"
              srcset="{%- if block.settings.block_img_2.width >= 275 -%}{{ block.settings.block_img_2 | image_url: width: 275 }} 275w,{%- endif -%}
              {%- if block.settings.block_img_2.width >= 550 -%}{{ block.settings.block_img_2 | image_url: width: 550 }} 550w,{%- endif -%}
              {%- if block.settings.block_img_2.width >= 710 -%}{{ block.settings.block_img_2 | image_url: width: 710 }} 710w,{%- endif -%}
              {%- if block.settings.block_img_2.width >= 1420 -%}{{ block.settings.block_img_2 | image_url: width: 1420 }} 1420w,{%- endif -%}
              {{ block.settings.block_img_2 | image_url }} {{ block.settings.block_img_2.width }}w"
              src="{{ block.settings.block_img_2 | image_url: width: 550 }}"
              sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
              (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %}, calc(100vw - 30px)"
              alt="{{ block.settings.block_img_2.alt }}"
              height="{{ block.settings.block_img_2.height }}"
              width="{{ block.settings.block_img_2.width }}"
              loading="lazy"
            >
          </div>
          {% endif %}
         
          {% endif %}
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>

<style>

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-heading .dt-sc-main-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_heading_color }}; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-heading .dt-sc-sub-heading { color: var(--DTColor_Heading); color:{{ section.settings.section_sub_heading_color }}; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-heading .dt-sc-heading-description { color: var(--DTColor_Body); color:{{ section.settings.section_description_color }}; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-heading .dt-sc-btn { 
    background: var(--DT_Button_BG_Color); background:{{ section.settings.section_button_bg_color }}; 
      color: var(--DT_Button_Text_Color); color:{{ section.settings.section_button_text_color }}; 
        }
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-heading .dt-sc-btn:hover { 
    background: var(--DT_Button_BG_Hover_Color); background:{{ section.settings.section_button_hover_bg_color }}; 
      color: var(--DT_Button_Text_Hover_Color); color:{{ section.settings.section_button_hover_text_color }}; 
        }

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-overlay:before  { 
    color: var(--DTTertiaryColor); 
      color: {{ section.settings.background_color }};    
      background: currentcolor; 
      opacity: {{ section.settings.overlay_transparency }}; 
      }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper { align-items: flex-start; }
  
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block { display: grid; gap: {{ section.settings.column_gap }}px; position: relative; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block .dt-sc-custom-content { grid-template-columns: repeat(2, 1fr); display: grid; gap: 30px; }
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block .dt-sc-heading { margin: 0; z-index: 1; }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block .dt-sc-custom-content .dt-sc-custom-image {
    position: relative; 
  }

  {% for block in section.blocks %} 

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-main-heading { 
    color: var(--DTColor_Heading); color:{{ block.settings.block_heading_color }}; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-sub-heading { 
    color: var(--DTColor_Heading); color:{{ block.settings.block_sub_heading_color }}; } 
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-heading-description { 
    color: var(--DTColor_Body); color:{{ block.settings.block_description_color }}; } 

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-heading-html { 
    color: var(--DTColor_Body); color:{{ block.settings.block_html_color }}; } 

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-btn { 
    background: var(--DT_Button_BG_Color); background:{{ block.settings.block_button_bg_color }}; 
      color: var(--DT_Button_Text_Color); color:{{ block.settings.block_button_text_color }}; 
        }
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading .dt-sc-btn:hover { 
    background: var(--DT_Button_BG_Hover_Color); background:{{ block.settings.block_button_hover_bg_color }}; 
      color: var(--DT_Button_Text_Hover_Color); color:{{ block.settings.block_button_hover_text_color }}; 
        }

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-heading 
  img.dt-sc-block-image:first-child { border-radius: var(--DTRadius); border-radius: {{ block.settings.image_1_border_radius }}; }

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} > img.dt-sc-block-image { 
    border-radius: var(--DTRadius); border-radius: {{ block.settings.image_2_border_radius }}; }

  
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-custom-content.align_middle {
    align-items: center;
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-custom-content.dt-sc-custom-image.align_bottom {
    align-items: flex-end;
  }

  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-custom-content .dt-sc-custom-image:only-child {
    grid-column: 1/3;
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }}.dt-sc-reverse-rows > .dt-sc-block-image,
  #shopify-section-{{ section.id }}.home-custom-section .dt-sc-custom-block.{{ block.type }}-{{ block.id }} .dt-sc-custom-content.dt-sc-reverse-columns .dt-sc-heading {
    order: -1;
  }
  
  {% unless section.settings.two_column %}
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block > .dt-sc-block-image {
    position: absolute;
    bottom: 0;
    right: 0;
    width: auto;
    height: 300px; 
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.dt-sc-reverse-rows > .dt-sc-block-image {
    left: 0; right: auto;
  }

  {% endunless %}
  
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}:before { 
    top: 50%; left: 2%; transform: translateY(-15%); -webkit-transform: translateY(-15%); width: 45%; height: 50%; 
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}:before,
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}:only-child:before {
    content: ""; display: block; position: absolute; 
    mask-image: url({{ block.settings.pattern_image | image_url: width: 1920 }}); -webkit-mask-image: url({{ block.settings.pattern_image | image_url: width: 1920 }});
    mask-size: 100%; mask-repeat: no-repeat; mask-position: center; -webkit-mask-size: 100%; -webkit-mask-repeat: no-repeat; -webkit-mask-position: center;
    z-index: -1; background:linear-gradient(-45deg, var(--DTPrimaryColor), var(--DTSecondaryColor), var(--DTTertiaryColor),var(--DTOverlayColor)); 
      background-size:400% 400%; animation:Gradient 3s linear infinite; -webkit-animation:Gradient 3s linear infinite;
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}.dt-sc-reverse-rows:before {
    left: auto; right: 2%; top: auto; bottom: 25%;
  }
  
  #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}:only-child:before {
  bottom: 0; right: -100px; left: auto; top: auto; width: 385px; height: 440px;
  }

  {% endfor %}  
  
  @keyframes Gradient{
    0%, 100%{background-position:0% 50%; opacity: 1; } 
    50%{background-position:100% 50%; opacity: 0.5; }
  }
  
  @-webkit-keyframes Gradient{
    0%, 100%{background-position:0% 50%; opacity: 1; } 
    50%{background-position:100% 50%; opacity: 0.5; }
  }


  @media (max-width: 1360px) {

    {% for block in section.blocks %}

      {% unless section.settings.two_column %}


      #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }} > .dt-sc-block-image,
      #shopify-section-{{ section.id }}.home-custom-section .home-custom-section-wrapper.custom_style .dt-sc-custom-block.{{ block.type }}-{{ block.id }}.dt-sc-reverse-rows > .dt-sc-block-image {
        height: 200px; 
      }

      {% endunless %}
    

    {% endfor %} 

  }

  @media only screen and (min-width: 992px) {
